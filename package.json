{
  "name": "@axin666/ai-ui",
  "version": "0.1.9",
  "description": "A Vue 3 component library for admin systems",
  "type": "module",
  "main": "dist/ai-ui.cjs.js",
  "module": "dist/ai-ui.esm.js",
  "types": "dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ai-ui.esm.js",
      "require": "./dist/ai-ui.cjs.js",
      "types": "./dist/types/src/index.d.ts"
    },
    "./Icon": {
      "import": "./dist/ai-ui.esm.js",
      "require": "./dist/ai-ui.cjs.js",
      "types": "./dist/types/src/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "dist/style.css",
    "packages/theme/index.scss",
    "packages/components/**/style.scss"
  ],
  "scripts": {
    "dev": "vite",
    "dev:playground": "vite",
    "build": "vite build --mode lib",
    "build:lib": "BUILD_MODE=lib vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint . --ext .vue,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{vue,ts,tsx,js,jsx,json,md,scss,css}\"",
    "prepare": "husky install",
    "type-check": "vue-tsc --noEmit",
    "release": "pnpm lint && pnpm test && pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitepress-demo-preview/component": "^2.6.1",
    "@vitepress-demo-preview/plugin": "^1.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "sass": "^1.69.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitepress": "^1.0.0",
    "vitest": "^1.0.0",
    "vue": "^3.4.0",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.0"
  },
  "lint-staged": {
    "*.{vue,ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,css,json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "vue3",
    "component-library",
    "typescript",
    "admin",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  }
}
